{
    "name": "EasyInsight",
    "description": "KPI Autocomplete Plugin!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["file:///Users/*", "https://outlook.office.com/mail/", "https://twitter.com/home"],
          "js": ["js/content-script.js", "js/side-panel.js", "js/popup.js"],
          "css": ["css/style.css"]
        }
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "tabs", "bookmarks"],
    "host_permissions": [
      "https://outlook.office.com/mail/",
      "file:///*/*",
      "https://twitter.com/home"
      ],
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    },
    "web_accessible_resources": [{
      "resources": ["/images/*", "/js/*", "popup.html"],
      "matches": ["<all_urls>"]
    }],
   "action": {}

}
